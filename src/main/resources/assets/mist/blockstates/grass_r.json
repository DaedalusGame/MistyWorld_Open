{
    "multipart": [
        {   "when":  { "humus": "0", "wet": "true", "growth": "true" },
            "apply": [
                { "model": "mist:grass_r_wet_0" },
                { "model": "mist:grass_r_wet_0", "y": 90 },
                { "model": "mist:grass_r_wet_0", "y": 180 },
                { "model": "mist:grass_r_wet_0", "y": 270 }
            ]
        },
        {   "when":  { "humus": "1", "wet": "true", "growth": "true" },
            "apply": [
                { "model": "mist:grass_r_wet_1" },
                { "model": "mist:grass_r_wet_1", "y": 90 },
                { "model": "mist:grass_r_wet_1", "y": 180 },
                { "model": "mist:grass_r_wet_1", "y": 270 }
            ]
        },
        {   "when":  { "humus": "2", "wet": "true", "growth": "true" },
            "apply": [
                { "model": "mist:grass_r_wet_2" },
                { "model": "mist:grass_r_wet_2", "y": 90 },
                { "model": "mist:grass_r_wet_2", "y": 180 },
                { "model": "mist:grass_r_wet_2", "y": 270 }
            ]
        },
        {   "when":  { "humus": "3", "wet": "true", "growth": "true" },
            "apply": [
                { "model": "mist:grass_r_wet_3" },
                { "model": "mist:grass_r_wet_3", "y": 90 },
                { "model": "mist:grass_r_wet_3", "y": 180 },
                { "model": "mist:grass_r_wet_3", "y": 270 }
            ]
        },
        {   "when":  { "humus": "0", "wet": "false", "growth": "true" },
            "apply": [
                { "model": "mist:grass_r_dry_0" },
                { "model": "mist:grass_r_dry_0", "y": 90 },
                { "model": "mist:grass_r_dry_0", "y": 180 },
                { "model": "mist:grass_r_dry_0", "y": 270 }
            ]
        },
        {   "when":  { "humus": "1", "wet": "false", "growth": "true" },
            "apply": [
                { "model": "mist:grass_r_dry_1" },
                { "model": "mist:grass_r_dry_1", "y": 90 },
                { "model": "mist:grass_r_dry_1", "y": 180 },
                { "model": "mist:grass_r_dry_1", "y": 270 }
            ]
        },
        {   "when":  { "humus": "2", "wet": "false", "growth": "true" },
            "apply": [
                { "model": "mist:grass_r_dry_2" },
                { "model": "mist:grass_r_dry_2", "y": 90 },
                { "model": "mist:grass_r_dry_2", "y": 180 },
                { "model": "mist:grass_r_dry_2", "y": 270 }
            ]
        },
        {   "when":  { "humus": "3", "wet": "false", "growth": "true" },
            "apply": [
                { "model": "mist:grass_r_dry_3" },
                { "model": "mist:grass_r_dry_3", "y": 90 },
                { "model": "mist:grass_r_dry_3", "y": 180 },
                { "model": "mist:grass_r_dry_3", "y": 270 }
            ]
        },
        {   "when":  { "humus": "0", "wet": "true", "growth": "false" },
            "apply": [
                { "model": "mist:dirt_r_wet_0" },
                { "model": "mist:dirt_r_wet_0", "y": 90 },
                { "model": "mist:dirt_r_wet_0", "y": 180 },
                { "model": "mist:dirt_r_wet_0", "y": 270 }
            ]
        },
        {   "when":  { "humus": "1", "wet": "true", "growth": "false" },
            "apply": [
                { "model": "mist:dirt_r_wet_1" },
                { "model": "mist:dirt_r_wet_1", "y": 90 },
                { "model": "mist:dirt_r_wet_1", "y": 180 },
                { "model": "mist:dirt_r_wet_1", "y": 270 }
            ]
        },
        {   "when":  { "humus": "2", "wet": "true", "growth": "false" },
            "apply": [
                { "model": "mist:dirt_r_wet_2" },
                { "model": "mist:dirt_r_wet_2", "y": 90 },
                { "model": "mist:dirt_r_wet_2", "y": 180 },
                { "model": "mist:dirt_r_wet_2", "y": 270 }
            ]
        },
        {   "when":  { "humus": "3", "wet": "true", "growth": "false" },
            "apply": [
                { "model": "mist:dirt_r_wet_3" },
                { "model": "mist:dirt_r_wet_3", "y": 90 },
                { "model": "mist:dirt_r_wet_3", "y": 180 },
                { "model": "mist:dirt_r_wet_3", "y": 270 }
            ]
        },
        {   "when":  { "humus": "0", "wet": "false", "growth": "false" },
            "apply":  [
                { "model": "mist:dirt_r_dry_0" },
                { "model": "mist:dirt_r_dry_0", "y": 90 },
                { "model": "mist:dirt_r_dry_0", "y": 180 },
                { "model": "mist:dirt_r_dry_0", "y": 270 }
            ]
        },
        {   "when":  { "humus": "1", "wet": "false", "growth": "false" },
            "apply": [
                { "model": "mist:dirt_r_dry_1" },
                { "model": "mist:dirt_r_dry_1", "y": 90 },
                { "model": "mist:dirt_r_dry_1", "y": 180 },
                { "model": "mist:dirt_r_dry_1", "y": 270 }
            ]
        },
        {   "when":  { "humus": "2", "wet": "false", "growth": "false" },
            "apply":  [
                { "model": "mist:dirt_r_dry_2" },
                { "model": "mist:dirt_r_dry_2", "y": 90 },
                { "model": "mist:dirt_r_dry_2", "y": 180 },
                { "model": "mist:dirt_r_dry_2", "y": 270 }
            ]
        },
        {   "when":  { "humus": "3", "wet": "false", "growth": "false" },
            "apply":  [
                { "model": "mist:dirt_r_dry_3" },
                { "model": "mist:dirt_r_dry_3", "y": 90 },
                { "model": "mist:dirt_r_dry_3", "y": 180 },
                { "model": "mist:dirt_r_dry_3", "y": 270 }
            ]
        },
        {   "when":  { "OR": [
                { "humus": "0", "growth": "true", "snowy": "true" },
                { "growth": "false", "snowy": "true" }
            ]},
            "apply": [
                { "model": "mist:grass_snow_y" },
                { "model": "mist:grass_snow_y", "y": 90 },
                { "model": "mist:grass_snow_y", "y": 180 },
                { "model": "mist:grass_snow_y", "y": 270 }
            ]
        },
        {   "when":  { "humus": "1|2|3", "growth": "true", "snowy": "true" },
            "apply": [
                { "model": "mist:grass_snow" },
                { "model": "mist:grass_snow", "y": 90 },
                { "model": "mist:grass_snow", "y": 180 },
                { "model": "mist:grass_snow", "y": 270 }
            ]
        },
        {   "when":  { "growth": "false", "snowy": "false" },
            "apply": [
                { "model": "mist:grass_top_y" },
                { "model": "mist:grass_top_y", "y": 90 },
                { "model": "mist:grass_top_y", "y": 180 },
                { "model": "mist:grass_top_y", "y": 270 }
            ]
        }
    ]
}